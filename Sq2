Section-B
1 Function

CREATE OR REPLACE FUNCTION calc_interest(p NUMBER,r NUMBER,t NUMBER)
RETURN NUMBER IS
BEGIN
RETURN (p*r*t)/100;
END;
/
2 Procedure

CREATE OR REPLACE PROCEDURE dept_salary(dno NUMBER)
IS
BEGIN
SELECT AVG(sal), MIN(sal), MAX(sal)
FROM emp WHERE deptno=dno;
END;
