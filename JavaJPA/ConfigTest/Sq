INSERT INTO account VALUES (3,'C','chennai','3333333333',18000);
INSERT INTO account VALUES (4,'D','mumbai','4444444444',25000);
24

UPDATE account SET city='banglore' WHERE accno=1;
25

DELETE FROM account WHERE accno=1;
Section-B
1 Function

CREATE OR REPLACE FUNCTION calc_interest(p NUMBER,r NUMBER,t NUMBER)
RETURN NUMBER IS
BEGIN
RETURN (p*r*t)/100;
END;
/
2 Procedure

CREATE OR REPLACE PROCEDURE dept_salary(dno NUMBER)
IS
BEGIN
SELECT AVG(sal), MIN(sal), MAX(sal)
FROM emp WHERE deptno=dno;
END;
/
