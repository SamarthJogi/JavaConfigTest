Set-1 : Section-A
1

SELECT ename, sal*0.12 AS bonus FROM emp;
2

SELECT ename FROM emp WHERE job='MANAGER' AND EXTRACT(YEAR FROM hiredate)=2018;
3

SELECT ename FROM emp WHERE ename LIKE 'T%';
4

SELECT ename FROM emp WHERE job='MANAGER' AND deptno<>30;
5

SELECT empno AS NUMBER, ename AS NAME FROM emp;
6

SELECT ename FROM emp WHERE comm>400;
7

SELECT ename FROM emp WHERE ename LIKE '%R%' AND sal BETWEEN 1000 AND 4500;
8

SELECT ename FROM emp WHERE SUBSTR(ename,3,1)='L';
9

SELECT e.ename, e.sal, d.dname, d.loc
FROM emp e LEFT OUTER JOIN dept d
ON e.deptno=d.deptno;
10

SELECT e.ename, e.sal, m.ename AS mgr
FROM emp e JOIN emp m
ON e.mgr=m.empno
WHERE e.sal>m.sal;
11

SELECT ename, INSTR(ename,'R') FROM emp WHERE ename LIKE '%R%';
12

SELECT DISTINCT d.dname, d.loc
FROM dept d JOIN emp e
ON d.deptno=e.deptno;
13

SELECT ename FROM emp WHERE comm > sal*0.20;
14

SELECT * FROM emp WHERE job='MANAGER'
UNION
SELECT * FROM emp WHERE job='CLERK' AND deptno=10;
15

SELECT * FROM emp WHERE job='MANAGER' AND deptno=10
UNION
SELECT * FROM emp WHERE job='CLERK' AND deptno=20;
16

SELECT * FROM emp WHERE job='MANAGER' AND deptno=10
UNION
SELECT * FROM emp WHERE job='CLERK' AND deptno=20
ORDER BY ename;
17

SELECT * FROM emp
WHERE sal >
(SELECT AVG(sal) FROM emp WHERE deptno=emp.deptno);
18

SELECT SYSDATE FROM dual;
19

SELECT ename, sal, deptno FROM emp ORDER BY ename;
20

CREATE TABLE account(
accno INT PRIMARY KEY,
accHoldername VARCHAR2(50) NOT NULL,
city VARCHAR2(20) DEFAULT 'mumbai'
CHECK (city IN ('banglore','chennai','mumbai')),
telephone_number CHAR(10),
balance DECIMAL(10,2) CHECK (balance>10000)
);
21

